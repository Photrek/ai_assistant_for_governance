{
  "name": "ai_assistant_for_governance",
  "version": "0.0.1",
  "description": "Empowering Team-Based dReps for Cardano",
  "main": "./out/main/index.js",
  "author": "bakon",
  "homepage": "https://github.com/bakon11/cardano-dapp-pluts-template",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "webscraper": "ts-node --project tsconfig.node.json wenscraper/webscraper.ts",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build:notypes": "electron-vite build && npm run update_agent",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "electron-vite build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "build:browser": "electron-vite build && electron-builder --browser",
    "update_agent": "rm -r /var/www/aiagent/out/* && cp -av out/renderer/* /var/www/aiagent/out/"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/inter": "^5.0.18",
    "@fontsource/space-grotesk": "^5.1.0",
    "@harmoniclabs/plu-ts": "0.9.0-dev2",
    "@langchain/core": "^0.3.40",
    "@langchain/langgraph": "^0.2.48",
    "@langchain/ollama": "^0.2.0",
    "@mui/icons-material": "^6.2.1",
    "@mui/joy": "^5.0.0-beta.51",
    "ai_agent_client": "file:../ai_agent_api/client/typescript/ai_agent_client.tgz",
    "axios": "^1.8.4",
    "bip38": "^3.1.1",
    "bip39": "^3.1.0",
    "cbor-x": "^1.6.0",
    "crypto-js": "^4.2.0",
    "electron-updater": "^6.1.7",
    "jsdom": "^26.0.0",
    "katex": "^0.16.21",
    "knex": "^3.1.0",
    "moment": "^2.24.0",
    "natural": "^8.0.1",
    "ollama": "^0.5.14",
    "openai": "^4.98.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^4.0.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-i18next": "^11.17.1",
    "react-katex": "^3.0.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^2.15.0",
    "rehype-raw": "^7.0.0",
    "reusable": "^1.0.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@mui/types": "^7.2.15",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^24.0.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-katex": "^3.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/recharts": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.2",
    "reusable": "^1.1.0",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-node-polyfills": "^0.22.0"
  }
}
